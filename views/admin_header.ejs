        <style>
            body>div {
                border: 0px solid grey;
            }

            html,
            body,
            #alert {
                height: 100%;
                width: 100%;
                margin: 0;
                padding: 0;
            }

            #alert {
                overflow: hidden;
                position: relative;
            }

            #hideMe {
                -webkit-animation: cssAnimation 5s forwards;
                animation: cssAnimation 5s forwards;
            }

            @keyframes cssAnimation {
                0% {
                    opacity: 1;
                }

                90% {
                    opacity: 1;
                }

                100% {
                    opacity: 0;
                }
            }

            @-webkit-keyframes cssAnimation {
                0% {
                    opacity: 1;
                }

                90% {
                    opacity: 1;
                }

                100% {
                    opacity: 0;
                }
            }
        </style>